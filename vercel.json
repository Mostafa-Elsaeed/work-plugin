{
    "version": 2,
  "env": {
       "EMAIL_USER": "@email-user",
       "EMAIL_PASSWORD": "@email-password",
       "SENDER_NAME": "@sender-name"
     },
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ]
        }
    ]
}